@using EYSE.Models

@model List<SP_Obtener_TipoEquipo_Result>
@{
    ViewBag.Title = "Index";
}

<center><h2>Administracion de Equipos</h2></center>
<div class="row mt-4 d-flex">
    
    <div class="col-lg-10 d-flex justify-content-end  mb-3">
        <a href="@Url.Action("AgregarEquipo")" class="btn btn-outline-success">Nuevo Equipo</a>
    </div>

</div>

<div class="row d-flex justify-content-center">
    <div class="col-8">

        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">#id</th>
                    <th scope="col">Nombre de Equipo</th>
                    <th scope="col">Descripcion</th>
                    <th scope="col">Opciones</th>
                </tr>
            </thead>
            <tbody>

                @{ foreach (var item in Model)
                    {

                        <tr>
                            <th scope="row">@item.IdTipoEquipo</th>
                            <td>@item.Equipo</td>
                            <td>@item.Descripcion</td>
                            <td><i class="">Delete</i></td>
                        </tr>

                    }
                }
            </tbody>
        </table>

    </div>

</div>

